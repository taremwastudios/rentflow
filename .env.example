# Authentication
AUTH_SECRET=your-auth-secret-here

# Database (for local SQLite - use these values for local dev)
DB_URL=file:./local.db
DB_TOKEN=dev-token

# NowPayments (Cryptocurrency Payment Gateway)
# Get your API keys from https://nowpayments.io/
NOWPAYMENTS_API_KEY=your-nowpayments-api-key
NOWPAYMENTS_PUBLIC_KEY=your-nowpayments-public-key
NOWPAYMENTS_IPN_KEY=your-nowpayments-ipn-key

# App URL (for payment callbacks)
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app
